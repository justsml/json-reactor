{
  "name": "json-editor",
  "version": "1.2.01",
  "repository": {
    "type": "git",
    "url": "https://github.com/justsml/json-editor.git"
  },
  "bugs": {
    "url": "https://github.com/justsml/json-editor/issues"
  },
  "homepage": "https://github.com/justsml/json-editor",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "description": "A JSON Object Editor Component. Pure JavaScript, NO jQuery. Built w/ WebPack. Wrappers For React, Angular, Dojo coming soon!",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && http-server -p 1337",
    "build-dev": "NODE_ENV=development webpack --config ./webpack.config.js",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js",
    "watch": "webpack --watch --config ./webpack.config.js",
    "compress": "uglifyjs --compress --mangle --screw-ie8 -o ./dist/json-editor.min.js -- ./dist/json-editor.bundle.js",
    "test-debug": "casperjs test --verbose --log-level=debug ./tests/e2e-tests.js",
    "test": "casperjs test --verbose ./tests/e2e-tests.js"
  },
  "author": "Dan Levy <Dan@DanLevy.net>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "casper": "0.1.1",
    "casperjs": "1.1.1",
    "css-loader": "0.23.1",
    "eslint": "2.9.0",
    "expose-loader": "0.7.1",
    "http-server": "0.9.0",
    "less": "2.6.1",
    "less-loader": "2.2.3",
    "postcss-loader": "0.9.1",
    "st": "1.1.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.0"
  }
}
